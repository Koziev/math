{
 "variables":
 {
  "n1": "random.randint(2, 10)",
  "n2": "random.randint(2, 10)",
  "delta": "random.randint(1, 4)",
  "n3": "n2+delta",
  "n4": "random.randint(2, 10)",
  "obj": "ch('заяц|зайчик|барсук|хомяк|глухарь|лисенок|волк|медвежонок|олененок|тигренок|львенок|осел')",
  "obj2": "ch('заяц|зайчик|барсук|хомяк|глухарь|лисенок|волк|медвежонок|олененок|тигренок|львенок|осел')",
  "loc": "ch('На лесной полянке|На берегу пруда|У пруда|На песчаном пляже у речки|Рядом с ручейком|В заброшенной хижине лесника|Недалеко от покрытого зеленью холма')",
  "v": "ch('сидело|грелось|спало|лежало|возлежало|отдыхало')"
 },
 
 "constraints":
 [
  "n1 != n2"
 ],
 
 "dialogue":
 [
  "{ch('Перепиши|Измени|Скорректируй')} {ch('предложение|текст')} «{loc} {v} {n1} {numcor(n1, obj, 'Nom')}» так, чтобы {gen_plur(obj)} стало {n2}.",
  "{loc} {v} {n2} {numcor(n2, obj, 'Nom')}",
  ["Увеличь число {gen_plur(obj)} {opt('еще')} на {delta}", "Пусть теперь количество {gen_plur(obj)} увеличится еще на {delta}"],
  "{loc} {v} {n3} {numcor(n3, obj, 'Nom')}",
  "Добавь в этот текст {n4} {numcor(n4, obj2, 'Acc')}",
  "!if obj != obj2 goto DIFF_OBJ",
  "{loc} {v} {n3+n4} {numcor(n3+n4, obj, 'Nom')}",
  "!goto EXIT",
  "!:DIFF_OBJ",
  "{loc} {v} {n3} {numcor(n3, obj, 'Nom')} и {n4} {numcor(n4, obj2, 'Nom')}"
 ]
}
