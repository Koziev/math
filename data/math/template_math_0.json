{
 "variables":
 {
  "x1": "random.randint(5, 10)",
  "z": "3*x1",
  "x2": "x1+random.randint(1, 5)",
  "z2": "3*x2"
 },
 
 "constraints":
 [
  "x1 != x2"
 ],
 
 "dialogue":
 [
  "Сколько голов у {x1} трехголовых Змеев Горынычей?",
  "У них {x1}*3={z} {numcor(z, 'голова', 'Nom')}",
  ["А у {x2} их сколько?", "Сколько их у {x2} Змеев?"],
  ["У {x2} Змеев Горынычей их {x2}*3={z2}", "{x2}*3={z2} {numcor(z2, 'голова', 'Nom')}"]
 ]
}
